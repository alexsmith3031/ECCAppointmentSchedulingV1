<p id="notice"><%= notice %></p>

<h1>Insurances</h1>

<table>
  <thead>
    <tr>
      <th>Ins name</th>
      <th>Provide number</th>
      <th>Claim number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @insurances.each do |insurance| %>
      <tr>
        <td><%= insurance.ins_name %></td>
        <td><%= insurance.provide_number %></td>
        <td><%= insurance.claim_number %></td>
        <td><%= link_to 'Show', insurance %></td>
        <td><%= link_to 'Edit', edit_insurance_path(insurance) %></td>
        <td><%= link_to 'Destroy', insurance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Insurance', new_insurance_path %>
